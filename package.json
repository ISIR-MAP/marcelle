{
  "name": "@marcellejs/core",
  "description": "Marcelle Core API",
  "version": "0.1.0-alpha.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcellejs/marcelle"
  },
  "author": {
    "name": "Jules FrancÌ§oise",
    "email": "me@julesfrancoise.com"
  },
  "keywords": [
    "marcelle",
    "interactive machine learning"
  ],
  "license": "MIT",
  "main": "dist/marcelle.bundle.esm.js",
  "module": "dist/marcelle.esm.js",
  "browser": "dist/marcelle.bundle.umd.js",
  "types": "types/index.d.ts",
  "exports": {
    "require": "./dist/marcelle.bundle.umd.js",
    "import": "./dist/marcelle.esm.js",
    "default": "./dist/marcelle.esm.js"
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "dev:nobundle": "rollup -c -w --environment NO_BUNDLE",
    "build": "rollup -c",
    "types:dev": "tsc --project tsconfig.json  -d --emitDeclarationOnly --outDir types --watch",
    "types:build": "tsc --project tsconfig.json  -d --emitDeclarationOnly --outDir types",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "serve:examples": "serve -c examples/serve.json",
    "lint": "eslint src",
    "clean": "rm -rf node_modules dist examples/rollup/node_modules examples/rollup/public"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^6.1.0",
    "@tensorflow/tfjs": "^2.7.0",
    "@types/blueimp-load-image": "^5.14.1",
    "@types/ndarray": "^1.0.8",
    "@types/ndarray-ops": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "autoprefixer": "^9",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "postcss": "^7",
    "postcss-load-config": "^3.0.0",
    "prettier": "^2.2.0",
    "rollup": "^2.33.3",
    "rollup-plugin-external-globals": "^0.6.1",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-modify": "^3.0.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-sizes": "^1.0.3",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "serve": "^11.3.2",
    "svelte": "^3.29.7",
    "svelte-preprocess": "^4.6.1",
    "svelte-range-slider-pips": "^1.5.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "4.0.5",
    "vuepress": "^1.7.1"
  },
  "dependencies": {
    "@feathersjs/authentication-client": "^4.5.10",
    "@feathersjs/client": "^4.5.10",
    "@feathersjs/feathers": "^4.5.10",
    "@feathersjs/socketio-client": "^4.5.10",
    "@most/adapter": "^1.0.0",
    "@most/core": "^1.6.1",
    "@most/scheduler": "^1.3.0",
    "@most/types": "^1.1.0",
    "@tensorflow-models/coco-ssd": "^2.1.0",
    "@tensorflow-models/knn-classifier": "^1.2.2",
    "@tensorflow-models/mobilenet": "^2.0.4",
    "@tensorflow/tfjs-backend-cpu": "^2.7.0",
    "@tensorflow/tfjs-backend-webgl": "^2.7.0",
    "@tensorflow/tfjs-converter": "^2.7.0",
    "@tensorflow/tfjs-core": "^2.7.0",
    "@tensorflow/tfjs-layers": "^2.7.0",
    "blueimp-load-image": "^5.14.0",
    "chart.js": "^3.0.0-beta.6",
    "chartjs-chart-matrix": "^1.0.0-beta",
    "dequal": "^2.0.2",
    "feathers-localstorage": "^5.1.1",
    "feathers-memory": "^4.1.0",
    "onnxjs": "^0.1.8",
    "socket.io-client": "^2.3.1"
  }
